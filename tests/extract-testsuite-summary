#! /usr/bin/env perl
# Extract the testsuite summary generated by the parallel-tests harness
# from the output of "make check".

use warnings FATAL => 'all';
use strict;

my $br = '=' x 76;
my @sections = ('');
while (<>)
  {
    push @sections, $_, '' if /$br/;
    $sections[-1] .= $_ if !/$br/;
  }
print @sections[1..$#sections-1];
